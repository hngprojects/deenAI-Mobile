{
  "expo": {
    "owner": "deenai",
    "name": "DeenAi",
    "slug": "DeenAi-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nooraimobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs your location to provide accurate prayer times for your area.",
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "api.ottoman.emerj.net": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.allcodez.DeenAiMobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundImage": "./assets/images/android-icon.png",
        "monochromeImage": "./assets/images/android-icon.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            { "scheme": "nooraimobile" },
            { "scheme": "https", "host": "auth.expo.io", "pathPrefix": "/@deenai/DeenAi-Mobile" }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.allcodez.DeenAiMobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/splash-icon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.363819299326-00i9lufgchdclqtqc6nn3kp1bu95ahkp"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "524cdef3-3a7b-4fe8-b570-ca1758a8424d"
      },
      "googleAuth": {
        "iosClientId": "dummy-ios-client-id.apps.googleusercontent.com",
        "androidClientId": "dummy-android-client-id.apps.googleusercontent.com"
      }
    }
  }
}
